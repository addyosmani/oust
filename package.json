{
  "name": "oust",
  "version": "2.0.4",
  "author": "Addy Osmani <addyosmani@gmail.com> (https://addyosmani.com/)",
  "license": "Apache-2.0",
  "description": "Extract URLs to stylesheets, scripts, links, images or HTML imports from HTML",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/addyosmani/oust.git"
  },
  "bugs": {
    "url": "https://github.com/addyosmani/oust/issues"
  },
  "homepage": "https://github.com/addyosmani/oust",
  "main": "index.js",
  "bin": {
    "oust": "bin/cli.js"
  },
  "files": [
    "bin/cli.js",
    "index.js"
  ],
  "scripts": {
    "lint": "xo",
    "fix": "xo --fix",
    "uvu": "uvu test --ignore fixtures",
    "test": "npm run uvu",
    "test:coverage": "c8 npm run uvu",
    "xo": "xo"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "c8": "^7.13.0",
    "uvu": "^0.5.6",
    "xo": "^0.52.3"
  },
  "keywords": [
    "stylesheet",
    "import",
    "script",
    "extract",
    "list"
  ],
  "engines": {
    "node": ">=12"
  },
  "xo": {
    "space": 2,
    "rules": {
      "capitalized-comments": "off",
      "unicorn/prefer-module": "off"
    }
  }
}
